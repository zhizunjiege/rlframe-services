    "update_after": 200,
    "update_online_every": 1,
    "update_target_every": 200,
    "seed": null
  },
  "sifunc": "",
  "oafunc": "",
  "rewfunc": "",
  "hooks": [
    {
      "name": "Training",
      "args": {
        "test_policy_every": 8,
        "test_policy_total": 2
      }
    },
    {
      "name": "Logging",
      "args": {
        "loglvl": "DEBUG",
        "terminal": true,
        "tensorboard": true
      }
    },
    {
      "name": "AutoSave",
      "args": {
        "per_steps": 1000,
        "per_episodes": 10
      }
    }
  ]
